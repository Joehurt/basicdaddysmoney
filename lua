local Tab = Window:NewTab("Toggles")
local Section = Tab:NewSection("Toggles")

Section:NewSlider("WalkSpeed", "Want to change your WalkSpeed?", 500, 16, function(v)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
end)

Section:NewSlider("JumpPower", "Want to change your JumpPower?", 500, 50, function(v)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
end)

Section:NewSlider("Gravity", "Want to change your Gravity?", 0, 196.2, function(v)
    game.Workspace.Gravity = v
end)

Section:NewKeybind("Change Keybind", "Hate the current keybind?", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)





